          "cph-ng.problem.defaultMemoryLimit": {
            "type": "number",
            "default": 512,
            "markdownDescription": "%cph-ng.configuration.problem.properties.defaultMemoryLimit.markdownDescription%"
          },
          "cph-ng.problem.foundMatchTestcaseBehavior": {
            "type": "string",
