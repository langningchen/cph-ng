      {
        "id": "runner",
        "order": 2,
        "title": "%cph-ng.configuration.runner.title%",
        "properties": {
          "cph-ng.runner.timeAddition": {
            "type": "number",
            "default": 1000,
            "markdownDescription": "%cph-ng.configuration.runner.properties.timeAddition.markdownDescription%"
          },
          "cph-ng.runner.useRunner": {
            "type": "boolean",
            "default": false,
            "markdownDescription": "%cph-ng.configuration.runner.properties.useRunner.markdownDescription%"
          },
          "cph-ng.runner.unlimitedStack": {
            "type": "boolean",
            "default": false,
            "markdownDescription": "%cph-ng.configuration.runner.properties.unlimitedStack.markdownDescription%"
          }
        }
      },
