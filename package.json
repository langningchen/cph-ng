          },
          "cph-ng.compilation.pythonRunner": {
            "type": "string",
            "default": "pypy3",
            "markdownDescription": "%cph-ng.configuration.compilation.properties.pythonRunner.markdownDescription%"
          },
          "cph-ng.compilation.pythonRunArgs": {
            "type": "string",
            "default": "",
            "markdownDescription": "%cph-ng.configuration.compilation.properties.pythonRunArgs.markdownDescription%"
          },
          "cph-ng.compilation.javascriptRunner": {
            "type": "string",
            "default": "node",
            "markdownDescription": "%cph-ng.configuration.compilation.properties.javascriptRunner.markdownDescription%"
          },
          "cph-ng.compilation.javascriptRunArgs": {
            "type": "string",
            "default": "",
